<?xml version="1.0" ?>
<ruleset name="OxyNET">
    <description>Les regles de formatage</description>

    <arg name="colors"/>
    <arg value="p"/>

    <file>src</file>
    <file>public/index.php</file>

    <rule ref="PSR2"></rule>
    <rule ref="Generic.Arrays.DisallowLongArraySyntax"></rule>

    <rule ref="Generic.Files.LineEndings"><exclude name="Generic.Files.LineEndings.InvalidEOLChar"/></rule>
</ruleset>